<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="css/forms.css">
    <link rel="stylesheet" href="css/bootstrap.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Form</title>
</head>

<body>
    <a class="backhome fixed-top" href="/admin">Home</a>
    <div class="color color-1"></div>
    <div class="color color-2"></div>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h1 id="mainheading">Noor-e-Ilam Admin Form </h1>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row main">
            <%for(let i=0 ; i<user.length ; i++) {%>
                <div id="rlt" class="col-6 formbox">
                    <a id="check" href="admindone?id=<%= user[i]._id %>">Add</a>
                    <a id="link" href="adminformremove?id=<%= user[i]._id %>">Remove</a>
                    <h1>Admin Data</h1>
                    <h2><span>Name:</span>
                        <%= user[i].name %>
                    </h2>
                    <h2><span>Email:</span>
                        <%= user[i].email %>
                    </h2>
                    <h2><span>Number:</span>
                        <%= user[i].number %>
                    </h2>
                </div>
                <%}%>
        </div>
        <div class="row">
            <div class="col-12">
                <h1 id="subheading">Finale Admin</h1>
            </div>
        </div>
        <div class="row main">
            <%for(let i=1 ; i<userreq.length ; i++) {%>
                <div id="rlt" class="col-6 ">
                    <a id="link" href="adminremove?id=<%= userreq[i]._id %>">Remove</a>
                    <h1>Admin</h1>
                    <h2><span>Name:</span>
                        <%= userreq[i].name %>
                    </h2>
                    <h2><span>Email:</span>
                        <%= userreq[i].email %>
                    </h2>
                    <h2><span>Number:</span>
                        <%= userreq[i].number %>
                    </h2>
                </div>
            <%}%>
        </div>
    </div>

</body>

</html>